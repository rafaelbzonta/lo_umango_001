sql

CREATE TABLE SCAN_PROCESSADO (
    ID INTEGER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
    DATA_HORA TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    USUARIO_SCAN VARCHAR(100),
    ORDEM_SERVICO VARCHAR(50),
    SETOR VARCHAR(50),
    EMAIL_CLIENTE VARCHAR(100),
    VALOR_OCR_ZONA VARCHAR(100)
);

SELECT 

USUARIO_SCAN, ID, DATA_HORA, PRIORIDADE, EMAIL_CLIENTE, SETOR, ORDEM_SERVICO, VALOR_OCR_ZONA 

FROM SCAN_PROCESSADO;
